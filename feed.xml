---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">{%- comment -%}<!-- Atom syndication format https://tools.ietf.org/html/rfc4287 -->{%- endcomment -%}
  {%- capture id_prefix -%}tag:{{ site.custom_feed_id_iri_email }},{{ site.custom_feed_id_iri_inception_year }}:{%- endcapture -%}
  <id>{{ id_prefix }}blog</id>{%- comment -%}<!-- tag URI https://tools.ietf.org/html/rfc4151 -->{%- endcomment -%}
  <link rel="self" type="application/atom+xml" href="{{ page.url | absolute_url }}"/>
  <link rel="alternate" type="text/html" href="{{ "" | absolute_url }}"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <title type="text">{{ site.custom_plaintext_title | xml_escape }}</title>
  <subtitle type="text">Latest blog posts</subtitle>
  {%- for post in site.posts limit: 50 -%}
    {%- unless post.draft -%}
      <entry>
        <id>{{ id_prefix }}blog/{{ post.slug }}</id>
        <link rel="alternate" type="text/html" href="{{ post.url | absolute_url }}"/>
        <published>{{ post.date | date_to_xmlschema }}</published>
        <updated>{{ post.custom_update_date | date_to_xmlschema }}</updated>
        <title type="html">{{ post.title | xml_escape }}</title>
        <author>
          <name>{{ site.custom_author }}</name>
          <email>{{ site.custom_author_email }}</email>
        </author>
      </entry>
    {%- endunless -%}
  {%- endfor -%}
</feed>
